<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chris.fitnessapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Profile">
        </activity>

        <activity
            android:name=".HomePage">
            android:label="@string/app_name">
        </activity>
        <activity android:name=".exersizeTemplate" />
        <activity android:name=".legsList" />
        <activity android:name=".absList"/>
        <activity android:name=".backList"/>
        <activity android:name=".calfList"/>
        <activity android:name=".chestList"/>
        <activity android:name=".forearmList"/>
        <activity android:name=".shoulderList"/>
        <activity android:name=".tricepList"/>

    </application>

</manifest>